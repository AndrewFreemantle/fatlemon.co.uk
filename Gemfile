source 'https://rubygems.org'

# Github Pages blog using Jekyll:  http://fatlemon.github.io/
gem 'jekyll'

# Markdown support by redcarpet
gem 'redcarpet'

# Pagination
gem 'jekyll-paginate'

# Handle rewrites / redirects (for post slugs)
gem 'jekyll-redirect-from'

# Syntax highlighting
gem 'pygments.rb'

# Emoji support
gem 'gemoji'

# Jekyll server: 0.0.0.0:4000
#  bundle exec jekyll serve
